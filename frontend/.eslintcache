\"\r\n              onChange={this.handleChange}\r\n              value={this.state.reviewToEdit.txt}\r\n            ></textarea>\r\n            <button>Submit</button>\r\n          </form>}\r\n        <hr />\r\n      </div>\r\n    )\r\n  }\r\n}\r\n\r\nconst mapStateToProps = state => {\r\n  return {\r\n    reviews: state.reviewModule.reviews,\r\n    users: state.userModule.users,\r\n    loggedInUser: state.userModule.loggedInUser\r\n  }\r\n}\r\nconst mapDispatchToProps = {\r\n  loadReviews,\r\n  loadUsers,\r\n  addReview,\r\n  // removeReview\r\n}\r\n\r\nexport const Home = connect(mapStateToProps, mapDispatchToProps)(_Home)\r\n","C:\\Users\\itzha\\Desktop\\dev\\coding-academy\\final\\MyArt\\frontend\\src\\pages\\Chat.jsx",[],"C:\\Users\\itzha\\Desktop\\dev\\coding-academy\\final\\MyArt\\frontend\\src\\pages\\AppStore.jsx",["115","116","117","118","119"],"C:\\Users\\itzha\\Desktop\\dev\\coding-academy\\final\\MyArt\\frontend\\src\\pages\\UserDetails.jsx",[],"C:\\Users\\itzha\\Desktop\\dev\\coding-academy\\final\\MyArt\\frontend\\src\\cmps\\Header.jsx",[],"C:\\Users\\itzha\\Desktop\\dev\\coding-academy\\final\\MyArt\\frontend\\src\\store\\reducers\\index.js",[],"C:\\Users\\itzha\\Desktop\\dev\\coding-academy\\final\\MyArt\\frontend\\src\\store\\actions\\reviewActions.js",[],"C:\\Users\\itzha\\Desktop\\dev\\coding-academy\\final\\MyArt\\frontend\\src\\services\\appStoreService.js",[],"C:\\Users\\itzha\\Desktop\\dev\\coding-academy\\final\\MyArt\\frontend\\src\\store\\actions\\userActions.js",[],"C:\\Users\\itzha\\Desktop\\dev\\coding-academy\\final\\MyArt\\frontend\\src\\services\\socketService.js",[],"C:\\Users\\itzha\\Desktop\\dev\\coding-academy\\final\\MyArt\\frontend\\src\\services\\userService.js",[],"C:\\Users\\itzha\\Desktop\\dev\\coding-academy\\final\\MyArt\\frontend\\src\\store\\reducers\\reviewReducer.js",[],"C:\\Users\\itzha\\Desktop\\dev\\coding-academy\\final\\MyArt\\frontend\\src\\store\\reducers\\userReducer.js",[],"C:\\Users\\itzha\\Desktop\\dev\\co